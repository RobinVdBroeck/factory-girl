{
  "name": "factory-girl",
  "description": "A factory library for Node.js and JavaScript inspired by factory_girl",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "author": [
    "@aexmachina",
    "@chetanism"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "transform-class-properties"
    ]
  },
  "version": "5.0.3",
  "keywords": [
    "factory",
    "test",
    "bdd",
    "tdd",
    "fixture"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/aexmachina/factory-girl.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "http://github.com/aexmachina/factory-girl/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/aexmachina/factory-girl/raw/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=18.12.0"
  },
  "dependencies": {
    "chance": "^1.0.4"
  },
  "devDependencies": {
    "@eslint/js": "~9.19.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "~9.19.0",
    "eslint-config-prettier": "~10.0.0",
    "eslint-plugin-mocha": "~10.5.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "pkgroll": "^2.6.0",
    "prettier": "^3.4.2",
    "should": "^6.0.3",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src test",
    "test": "NODE_ENV=test mocha",
    "test:cover": "NODE_ENV=test babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "build": "pkgroll --target=node18 --sourcemap"
  }
}
